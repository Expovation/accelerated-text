<ccg-lexicon name="shop" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:noNamespaceSchemaLocation="../lexicon.xsd">

  <family name="GPE" pos="NNP">
    <entry name="Primary">
      <atomcat type="n">
        <lf>
          <satop nomvar="X:gpe">
            <prop name="[*DEFAULT*]"/>
          </satop>
        </lf>
      </atomcat>
    </entry>
  </family>

  <family name="Conflict" pos="V" closed="false">
    <entry name="Primary">
      <complexcat>
        <atomcat type="s">
