        </atomcat>
        <lf>
          <satop nomvar="E">
            <prop name="[*DEFAULT*]"/>
          </satop>
        </lf>
      </complexcat>
    </entry>
  </family>

  <family name="whith-whom" pos="IN">
    <entry name="Primary">
      <complexcat>
        <atomcat type="s"/>
        <slash mode="*" dir="\"/>
        <atomcat type="s"/>
        <slash mode="*" dir="/"/>
        <atomcat type="n"/>
        <lf>
          <satop nomvar="E">
            <prop name="[*DEFAULT*]"/>
          </satop>
        </lf>
      </complexcat>
    </entry>
  </family>
